            <div class="content mt-3">
                <div class="animated fadeIn">
                    <div class="row">

                        <div class="col-md-12">
                            <div class="card">
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-sm-8">
                                            <h4 class="card-title mb-0">Devices</h4>
                                            <div class="small text-muted">Click on a device to track it in realtime.</div>
                                        </div>
                                    </div>
                                    <div class="pt-3 container">
                                        <table id="bootstrap-data-table" class="table table-striped table-bordered">
                                            <thead>
                                                <tr>
                                                    <th>Name</th>
                                                    <th>Last seen</th>
                                                    <th>Location</th>
                                                    <th>Probability</th>
                                                    <th>Active time (min)</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                {{ range .Devices }}
                                                <tr>
                                                    <td>
                                                        <a href="/view/location/{{$.Family}}/{{.Name}}">{{ .Name }}</a>
                                                    </td>
                                                    <td id="lastseen-{{.ID}}">{{ .LastSeen.Format "Mon, 2 Jan 2006 15:04:05" }}</td>
                                                    <td id="location-{{.ID}}">{{ .LastLocation }}</td>
                                                    <td id="probability-{{.ID}}">{{ .Probability }}%</td>
                                                    <td>{{ .ActiveTime }}</td>
                                                </tr>
                                                {{ end }}

                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>


                    </div>
                </div>
                <!-- .animated -->
            </div>
            <!-- .content -->

